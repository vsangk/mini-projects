<p id="notice"><%= notice %></p>

<h1>Secured Resouces</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @secured_resouces.each do |secured_resouce| %>
      <tr>
        <td><%= link_to 'Show', secured_resouce %></td>
        <td><%= link_to 'Edit', edit_secured_resouce_path(secured_resouce) %></td>
        <td><%= link_to 'Destroy', secured_resouce, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

